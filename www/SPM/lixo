<script>
jQuery("form.whoisForm").submit(function (e) {
  var domain = jQuery("#domain-search-name").val();
  console.log(domain);
  if(!domain.includes(".")){
    jQuery("#domain-search-name").val(domain+".pt");
  }
});
</script>

